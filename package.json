{
  "name": "Aldo",
  "version": "0.0.1",
  "description": "Collecting faces for fun and profit.",
  "scripts": {
    "build": "concurrently \"npm run sass\" \"npm run compile\"",
    "build:watch": "concurrently \"npm run sass:watch\" \"npm run compile:watch\"",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "sass": "node-sass src/app/ -o src/app",
    "sass:watch": "nodemon",
    "compile": "tsc -p src/",
    "compile:watch": "tsc -w -p src/"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/router": "^4.4.5",
    "@covalent/core": "1.0.0-beta.8-1",
    "core-js": "^2.5.1",
    "hammerjs": "~2.0.8",
    "node-sass": "~4.5.3",
    "rxjs": "~5.4.3",
    "systemjs": "0.19.40",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@types/node": "^6.0.89",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "lite-server": "^2.2.2",
    "lodash": "^4.17.21",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.6.0",
    "typescript": "~2.4.0"
  },
  "repository": {}
}
